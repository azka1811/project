<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="600.0">
         <children>
            <BorderPane prefHeight="600.0" prefWidth="600.0">
               <left>
                  <VBox prefHeight="606.0" prefWidth="150.0" style="-fx-background-color: #6872EC;" BorderPane.alignment="CENTER">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </left>
               <top>
                  <HBox prefHeight="130.0" prefWidth="600.0" style="-fx-background-color: #6872EC;" BorderPane.alignment="CENTER">
                     <children>
                        <Button fx:id="backDeleteMedicine" mnemonicParsing="false" onAction="#backFromDeleteMedicineFunc" prefHeight="121.0" prefWidth="289.0" style="-fx-background-color: #6872EC;" text="back" textFill="WHITE">
                           <font>
                              <Font size="25.0" />
                           </font>
                        </Button>
                        <Label prefHeight="129.0" prefWidth="601.0" text="                                   DELETE MEDICINE" textFill="#fcfcfc">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </top>
            </BorderPane>
            <TextField fx:id="medicineID" layoutX="240.0" layoutY="221.0" promptText="Medicine ID" />
            <TextField fx:id="medicineName" layoutX="240.0" layoutY="260.0" promptText="Medicine Quantity" />
            <Button fx:id="remove" layoutX="284.0" layoutY="405.0" mnemonicParsing="false" onAction="#RemoveMedicineFunction" text="-">
               <font>
                  <Font size="30.0" />
               </font>
            </Button>
            <Label fx:id="medicineAddedSuccessfully" layoutX="301.0" layoutY="499.0" text="Label" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>

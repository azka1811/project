<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-image: application\e.jpg;" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="600.0" prefWidth="191.0" style="-fx-background-color: #6872EC;">
               <children>
                  <Pane prefHeight="149.0" prefWidth="225.0">
                     <children>
                        <Button fx:id="BackFromSalesPerson" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackFromSalesPersonView" text="back" />
                     </children></Pane>
                  <Button fx:id="StartNewSaleButton" mnemonicParsing="false" onAction="#StartNewSaleButton" prefHeight="55.0" prefWidth="225.0" style="-fx-background-color: #6872EC;" text="START NEW SALE" textFill="WHITE">
                     <font>
                        <Font name="Berlin Sans FB" size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="Return" mnemonicParsing="false" onAction="#HandleReturn" prefHeight="55.0" prefWidth="225.0" style="-fx-background-color: #6872EC;" text="RETURN MEDICINE" textFill="#f8f8f8">
                     <font>
                        <Font name="Berlin Sans FB" size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="ViewSalesButtonSalesPerson" mnemonicParsing="false" onAction="#ViewSalesAsSalesPerson" prefHeight="55.0" prefWidth="225.0" style="-fx-background-color: #6872EC;" text="VIEW SALES" textFill="WHITE">
                     <font>
                        <Font name="Berlin Sans FB" size="19.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children></AnchorPane>
   </center>
</BorderPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-image: application\e.jpg;" BorderPane.alignment="CENTER">
         <children>
            <VBox prefHeight="600.0" prefWidth="191.0" style="-fx-background-color: #6872EC;">
               <children>
                  <Pane prefHeight="135.0" prefWidth="442.0" />
                  <Button fx:id="AddMedicineButton" mnemonicParsing="false" onAction="#addNewMedicineFunc" prefHeight="100.0" prefWidth="191.0" style="-fx-background-color: #6872EC;" text="ADD MEDICINE" textFill="WHITE">
                     <font>
                        <Font name="Berlin Sans FB" size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="DeleteMedicineButton" mnemonicParsing="false" onAction="#DeleteMedicine" prefHeight="80.0" prefWidth="191.0" style="-fx-background-color: #6872EC;" text="REMOVE MEDICINE" textFill="#f8f8f8">
                     <font>
                        <Font name="Berlin Sans FB" size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="ViewMedicinesButton" mnemonicParsing="false" onAction="#ViewSales" prefHeight="82.0" prefWidth="191.0" style="-fx-background-color: #6872EC;" text="VIEW SALES" textFill="WHITE">
                     <font>
                        <Font name="Berlin Sans FB" size="19.0" />
                     </font>
                  </Button>
                  <Button fx:id="catalogview" mnemonicParsing="false" onAction="#viewCatalogFunc" prefHeight="82.0" prefWidth="191.0" style="-fx-background-color: #6872EC;" text="View Catalog" textFill="WHITE">
                     <font>
                        <Font name="Berlin Sans FB" size="19.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <HBox layoutX="-1.0" prefHeight="122.0" prefWidth="600.0" style="-fx-background-color: #6872EC;">
               <children>
                  <Label prefHeight="122.0" prefWidth="603.0" text="                                                                  WELCOME MANAGER" />
               </children>
            </HBox>
            <Button fx:id="BackButtonManagerView" layoutX="490.0" layoutY="554.0" mnemonicParsing="false" onAction="#backFromManagerFunc" prefHeight="32.0" prefWidth="96.0" text="BACK" />
         </children></AnchorPane>
   </center>
</BorderPane>
